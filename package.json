{
    "name": "snotes",
    "version": "1.0.0",
    "description": "A new way to take notes",
    "main": "app/main.js",
    "scripts": {
        "dev": "webpack -d",
        "prod": "webpack -p",
        "start": "electron .",
        "dist": "build --windows --ia32 --x64",
        "sw": "node build.sw.js"
    },
    "author": "Simon Loir",
    "license": "MIT",
    "dependencies": {
        "electron-spell-check-provider": "^1.1.1"
    },
    "devDependencies": {
        "css-loader": "^0.28.11",
        "electron": "^1.8.8",
        "electron-builder": "^19.56.2",
        "electron-rebuild": "^1.8.2",
        "node-sass": "^4.9.3",
        "sass-loader": "^6.0.7",
        "style-loader": "^0.20.3",
        "three": "^0.91.0",
        "ts-loader": "^4.5.0",
        "typescript": "^2.9.2",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.0"
    },
    "build": {
        "appId": "simonloir.snotes",
        "win": {
            "target": "nsis",
            "icon": "./src/images/favicon.ico"
        },
        "directories": {
            "output": "builds"
        },
        "files": [
            {
                "filter": "!firebase-bot/**/*"
            }
        ]
    }
}
